      "init_arg": "(opt record { new_flow_origins = opt vec { \"http://uqqxf-5h777-77774-qaaaa-cai.localhost:4943\" }; captcha_config = opt record { max_unsolved_captchas = 50:nat64; captcha_trigger = variant { Static = variant { CaptchaDisabled } } } })",
